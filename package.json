{
  "name": "student-dashboard",
  "version": "1.0.0",
  "description": "SFU Student Dashboard application",
  "main": "bin/server.js",
  "scripts": {
    "start": "node ./bin/server.js",
    "lint": "eslint -c .eslintrc . bin src",
    "lint:fix": "npm run lint -- --fix",
    "lint:changed": "./scripts/lint-changed.sh",
    "test": "node_modules/.bin/ava -v --require babel-register src/**/__test__/*.js",
    "test:watch": "npm run test -- -w",
    "deps:reinstall": "rimraf node_modules && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfu/student-dashboard.git"
  },
  "author": "Graham Ballantyne <grahamb@sfu.ca>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sfu/student-dashboard/issues"
  },
  "homepage": "https://github.com/sfu/student-dashboard#readme",
  "pre-commit": [
    "lint:changed"
  ],
  "devDependencies": {
    "ava": "0.14.0",
    "babel-core": "6.7.2",
    "babel-eslint": "6.0.3",
    "babel-loader": "6.2.4",
    "babel-plugin-syntax-async-functions": "6.5.0",
    "babel-plugin-transform-regenerator": "6.6.5",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "eslint": "2.4.0",
    "eslint-plugin-react": "4.2.3",
    "fakeredis": "1.0.3",
    "piping": "0.3.0",
    "pre-commit": "1.1.2",
    "rimraf": "2.5.2",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "axios": "0.9.1",
    "body-parser": "1.15.0",
    "cas": "grahamb/node-cas",
    "connect-redis": "3.0.2",
    "cookie-parser": "1.4.1",
    "dotenv": "2.0.0",
    "eslint-plugin-react": "4.2.3",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "helmet": "1.3.0",
    "qs": "6.1.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "uid-safe": "2.1.0"
  }
}
